{
  "description": "A component for displaying tabbed content.",
  "tags": {},
  "displayName": "NeonTabs",
  "exportName": "default",
  "props": [
    {
      "name": "tabs",
      "tags": {},
      "description": "The list of tabs to display.",
      "type": {
        "name": "Array",
        "elements": [
          {
            "name": "NeonTabModel"
          }
        ]
      },
      "required": true
    },
    {
      "name": "value",
      "tags": {},
      "description": "The key of the selected tab.",
      "type": {
        "name": "string"
      },
      "required": true
    },
    {
      "name": "color",
      "tags": {},
      "description": "The tab highlight color.",
      "type": {
        "name": "NeonFunctionalColor"
      },
      "defaultValue": {
        "func": false,
        "value": "NeonFunctionalColor.Primary"
      }
    },
    {
      "name": "size",
      "tags": {},
      "description": "The tab size.",
      "type": {
        "name": "NeonSize"
      },
      "defaultValue": {
        "func": false,
        "value": "NeonSize.Medium"
      }
    }
  ],
  "events": [
    {
      "name": "input",
      "description": "Emitted when the selected tab is changed.",
      "type": {
        "names": [
          "string"
        ]
      }
    }
  ],
  "slots": [
    {
      "name": "default",
      "description": "Add tabs here (NeonTab). NOTE: Tabs can also be added outside of the component if required"
    }
  ]
}