{
  "description": "Defines a file drop zone.",
  "tags": {},
  "displayName": "NeonDropZone",
  "exportName": "default",
  "props": [
    {
      "name": "state",
      "tags": {},
      "description": "The state of the drop zone, used to indicate loading.",
      "type": {
        "name": "union",
        "elements": [
          {
            "name": "TSTypeReference"
          },
          {
            "name": "TSTypeReference"
          }
        ]
      },
      "defaultValue": {
        "func": false,
        "value": "NeonState.Ready"
      }
    },
    {
      "name": "disabled",
      "tags": {},
      "description": "The disabled state of the drop zone",
      "type": {
        "name": "boolean"
      },
      "defaultValue": {
        "func": false,
        "value": "false"
      }
    },
    {
      "name": "circular",
      "tags": {},
      "description": "Display the drop zone as a circle instead of square",
      "type": {
        "name": "boolean"
      }
    }
  ],
  "events": [
    {
      "name": "files",
      "description": "Emitted when files are dropped on the drop zone",
      "type": {
        "names": [
          "FileList"
        ]
      }
    }
  ],
  "slots": [
    {
      "name": "default"
    }
  ]
}